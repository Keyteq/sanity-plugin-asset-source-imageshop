{
  "name": "@keyteq/sanity-plugin-asset-source-imageshop",
  "version": "1.0.0",
  "description": "Sanity Studio plugin for using ImageShop to select images from",
  "keywords": [
    "sanity",
    "plugin",
    "images",
    "imageshop",
    "assets"
  ],
  "main": "dist/sanity-plugin-asset-source-imageshop.umd.js",
  "module": "dist/sanity-plugin-asset-source-imageshop.es5.js",
  "typings": "dist/types/sanity-plugin-asset-source-imageshop.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "config.dist.json"
  ],
  "author": "Keyteq <hei@keyteq.no>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Keyteq/sanity-plugin-asset-source-imageshop"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/Keyteq/sanity-plugin-asset-source-imageshop/issues"
  },
  "homepage": "https://github.com/Keyteq/sanity-plugin-asset-source-imageshop/#readme",
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && npm run postcss:build && typedoc --out docs --target es6 --theme minimal --mode file src",
    "watch-tsc": "tsc --watch --module commonjs",
    "watch-css": "postcss src/**/*.css --base src --dir dist/lib --watch",
    "watch-rollup": "rollup -c rollup.config.ts -w",
    "postcss:build": "postcss src/**/*.css --base src --dir dist/lib",
    "watch": "rollup -c rollup.config.ts && run-p watch-*",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "prepublishOnly": "npm run build",
    "prepush": "npm run test:prod && npm run build",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "@sanity/base": ">= 0.135.0",
    "@sanity/components": ">= 0.135.0",
    "react": ">= 16.2",
    "react-dom": ">= 16.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^7.6.0",
    "@sanity/base": "^0.140.36",
    "@sanity/components": "^0.140.38",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.17.4",
    "@types/react": "^16.9.11",
    "colors": "^1.4.0",
    "commitizen": "^4.2.1",
    "coveralls": "^3.0.7",
    "cross-env": "^5.2.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.0.1",
    "jest": "^26.4.2",
    "jest-config": "^26.4.2",
    "lint-staged": "^8.2.1",
    "lodash.camelcase": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.2",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "prompt": "^1.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-hot-loader": "^4.12.15",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.7.1",
    "rollup": "^2.27.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss-modules": "^2.0.2",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "semantic-release": "^15.13.30",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths-jest": "0.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
